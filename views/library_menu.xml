<odoo>
    <!--Library App Menu -->
    <menuitem id="library_menu" name="Library"/>

    <!-- Action to open the Book list -->
    <record id="action_library_book" model="ir.actions.act_window">
        <field name="name">Library Books</field>
        <field name="res_model">library.book</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record id="action_library_pagina1" model="ir.actions.act_window">
        <field name="name">Pagina 1</field>
        <field name="res_model">res.groups</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record id="action_library_pagina2" model="ir.actions.act_window">
        <field name="name">Pagina 2</field>
        <field name="res_model">res.partner</field>
        <field name="view_mode">tree,form</field>
    </record>

    <!-- Menu item to open the Book list -->

    <menuitem id="menu_library_book"
              name="Books"
              action="action_library_book"
              parent="library_menu" />
    <menuitem id="menu_library_ejercicio" name="Ejercicio" parent="library_menu"/>
    <menuitem id="menu_library_pagina1"
              name="Pagina1"
              action="action_library_pagina1"
              parent="menu_library_ejercicio" />
    <menuitem id="menu_library_pagina2"
              name="Pagina2"
              action="action_library_pagina2"
              parent="menu_library_ejercicio" />
</odoo>